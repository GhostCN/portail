        <li class="debutlii dropdown">
            <a class="nav-link dropdown-toggle dropb " data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false"><b>{{ title }}</b></a>
            <div class="dropdown-menu dropdow-menu-prinicepale">
                {% for offre in offres %}
                    {% if offre.title | lower == 'family' %}
                        <a class="dropdown-item" href="{{ path('offre-family') }}">{{ offre.title }}</a>
                    {% elseif offre.title | lower == 'premium' %}
                        <!--a class="dropdown-item" href="#">{{ offre.title }}</a-->
                    {% elseif offre.title | lower == 'international' %}
                        <a class="dropdown-item" href="{{ path('hub-pass-internationaux') }}">{{ offre.title }}</a>
                    {% else %}
                        <a class="dropdown-item" href="{{ path('details-offre-mobile',{'id': offre.url}) }}">{{ offre.title }}</a>
                    {% endif %}
                {% endfor %}
            </div>
        </li>
    </ul>
</div>

