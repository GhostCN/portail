                    {% set valeur = questions|length %}
                    {% set key = 1 %}
                    {% for question in questions %}
                    <div class="card {% if valeur == key %}border-0{% endif %}" {% if key == 1 %} style="border-top: none;"{% endif %}>
                        <div class="card-header my-2" id="heading{{ loop.index }}-3">
                        <h5 class="mb-0">
                            <button class="font20 btn-accordeon btn btn-link collapsed backg {% if loop.index > 1 %}collapsed{% endif %}" type="button" data-toggle="collapse" data-target="#collapse{{ loop.index }}-3" aria-expanded="{% if loop.index == 1 %}true{% else %} false{% endif %}" aria-controls="collapse{{ loop.index }}-3">
                                {{ question.titre_question }}
                            </button>
                        </h5>
                        </div>

                        <div id="collapse{{ loop.index }}-3" class="collapse {% if loop.index == 1 %}show{% endif %}" aria-labelledby="heading{{ loop.index }}-3" data-parent="#accordionExample-3">
                        <div class="card-body">
                            {{ question.reponse_question | raw }}
                        </div>
                        </div>
                    </div>
                        {% set key= key + 1 %}
                    {% endfor %}

                </div>
            </div>
        </div>
    </div>
</div>

